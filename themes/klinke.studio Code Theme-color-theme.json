    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": ["comment.documentation", "comment.block.documentation"],
            "settings": {
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#a8171e"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage"],
            "settings": {
                "foreground": "#004888"
            }
        },
        {
            "name": "Types",
            "scope": ["storage.type", "support.type"],
            "settings": {
                "foreground": "#0C0D0F"
            }
        },
        {
            "name": "Language Constants",
            "scope": ["constant.language", "support.constant", "variable.language"],
            "settings": {
                "foreground": "#004888"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "support.variable"],
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#0C0D0F"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type", "entity.other.inherited-class", "support.class"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#004888"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#a8171e"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": ["constant.numeric", "constant.character", "constant"],
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#007f53"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#007f53"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#d76023"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "HTML: Doctype Declaration",
            "scope": [
                "meta.tag.sgml.doctype",
                "meta.tag.sgml.doctype string",
                "meta.tag.sgml.doctype entity.name.tag",
                "meta.tag.sgml punctuation.definition.tag.html"
            ],
            "settings": {
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": ["meta.tag entity.other.attribute-name", "entity.other.attribute-name.html"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#004888"
            }
        },
        {
            "name": "HTML: Entities",
            "scope": ["constant.character.entity", "punctuation.definition.entity"],
            "settings": {
                "foreground": "#004888"
            }
        },
        {
            "name": "CSS: Selectors",
            "scope": [
                "meta.selector",
                "meta.selector entity",
                "meta.selector entity punctuation",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#0C0D0F"
            }
        },
        {
            "name": "CSS: Property Names",
            "scope": ["meta.property-name", "support.type.property-name"],
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "CSS: Property Values",
            "scope": [
                "meta.property-value",
                "meta.property-value constant.other",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "CSS: Important Keyword",
            "scope": "keyword.other.important",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: Error",
            "scope": "markup.error",
            "settings": {
                "foreground": "#a8171e"
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#007f53"
            }
        },
        {
            "name": "Markup: Output",
            "scope": ["markup.output", "markup.raw"],
            "settings": {
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "Markup: Prompt",
            "scope": "markup.prompt",
            "settings": {
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#0C0D0F"
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Traceback",
            "scope": "markup.traceback",
            "settings": {
                "foreground": "#a8171e"
            }
        },
        {
            "name": "Markup: Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#c7c6c6"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Markup Styling",
            "scope": ["markup.bold", "markup.italic"],
            "settings": {
                "foreground": "#007f53"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#004888"
            }
        },
        {
            "name": "Extra: Diff Range",
            "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Extra: Diff From",
            "scope": "meta.diff.header.from-file",
            "settings": {
                "foreground": "#2f3233"
            }
        },
        {
            "name": "Extra: Diff To",
            "scope": "meta.diff.header.to-file",
            "settings": {
                "foreground": "#2f3233"
            }
        }
    ]
